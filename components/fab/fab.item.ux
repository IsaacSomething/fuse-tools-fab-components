<Circle ux:Class="fab.fabItem" Width="40" Height="40" Opacity="0" Color="#fff"> 

    <string ux:Property="FabIcon" />
    <float4 ux:Property="FabColor" />

    <Text Font="FabIconFont" Alignment="Center" Value="{ReadProperty FabIcon}" Color="{ReadProperty FabColor}"/>
    <DropShadow Size="5" Distance="2" Color="#c5c5c5" Angle="90" />

</Circle>